{
	"root": true,
	"env": { "es2022": true },
	"parserOptions": { "ecmaVersion": 13 },
	"extends": [
		"eslint:recommended",
		"plugin:import/recommended",
		"airbnb-base",
		"plugin:jest/recommended",
		"prettier"
	],
	"rules": { "no-console": "off" },
	"ignorePatterns": ["/node_modules/", "/dist/", "/openapi/", "/static/"]
}
